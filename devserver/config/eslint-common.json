{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
    },
    "sourceType": "module"
  },
  "rules": {
    "semi": "off",
    "eol-last": "off",
    "comma-dangle": "off"
  }
}